,{"source":"sass/main.scss","name":null,"original":{"line":47,"column":4},"generated":{"line":298,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":330,"column":0},"generated":{"line":301,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":331,"column":2},"generated":{"line":302,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":332,"column":2},"generated":{"line":304,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":333,"column":4},"generated":{"line":305,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":336,"column":0},"generated":{"line":308,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":338,"column":0},"generated":{"line":309,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":339,"column":0},"generated":{"line":310,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":15,"column":2},"generated":{"line":311,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":16,"column":2},"generated":{"line":312,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":17,"column":2},"generated":{"line":313,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":18,"column":2},"generated":{"line":314,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":19,"column":2},"generated":{"line":315,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":20,"column":2},"generated":{"line":316,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":21,"column":2},"generated":{"line":317,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":22,"column":2},"generated":{"line":318,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":23,"column":2},"generated":{"line":319,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":24,"column":2},"generated":{"line":320,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":341,"column":2},"generated":{"line":321,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":343,"column":2},"generated":{"line":323,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":344,"column":4},"generated":{"line":324,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":345,"column":4},"generated":{"line":325,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":346,"column":4},"generated":{"line":326,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":29,"column":2},"generated":{"line":328,"column":0}},{"source":"sass/main.scss","name":null,"original":{"line":30,"column":4},"generated":{"line":329,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":31,"column":4},"generated":{"line":330,"column":2}},{"source":"sass/main.scss","name":null,"original":{"line":351,"column":0},"generated":{"line":333,"column":0}}],"sources":{"sass/main.scss":"@import url('https://fonts.googleapis.com/css?family=Abril+Fatface|Anton|Open+Sans&display=swap');\n\n// imports\n@import './resets';\n\n// flex center\n@mixin flex_center {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n// nav and footer style\n@mixin nav_and_footer {\n  height: 70px;\n  width: 100vw;\n  display: flex;\n  align-items: center;\n  font-size: larger;\n  color: rgb(41, 41, 41);\n  background-color: rgb(255, 255, 255);\n  box-shadow: 0px 0px 7px 1px rgb(196, 196, 196);\n  padding: 0px 40px 0px 40px;\n  z-index: 1;\n}\n\n// icon hover\n@mixin icon_hover {\n  &:hover {\n    cursor: pointer;\n    transform: scale(1.1);\n  }\n}\n\n// button style\n@mixin btn_style {\n  color: white;\n  background-color: black;\n  text-align: center;\n  padding: 15px 0px 15px 0px;\n  border-radius: 35px;\n  transition: 0.3s linear;\n  font-size: larger;\n  cursor: pointer;\n\n  &:hover {\n    background-color: rgb(70, 70, 70);\n  }\n}\n\n/*** nav ****/\n\n.nav {\n  @include nav_and_footer;\n  position: sticky;\n  top: 0;\n  justify-content: space-between;\n\n  .shopping-cart-div {\n    position: relative;\n    transition: 0.3s linear;\n    display: grid;\n    align-items: center;\n    justify-content: center;\n\n    @include icon_hover;\n\n    .label {\n      position: absolute;\n      bottom: 21px;\n      left: 23px;\n      background: rgb(255, 0, 76);\n      color: white;\n      font-size: small;\n      padding: 0px 4px 0px 4px;\n      border-radius: 25px 25px 25px 0px;\n    }\n  }\n\n  .fa-bars {\n    @extend .shopping-cart-div;\n  }\n}\n\n/* slider video */\n\n@mixin screen_style {\n  padding: 50px;\n  width: 100%;\n  margin: auto;\n}\n#video-big-screen {\n  @include screen_style;\n}\n#video-small-screen {\n  display: none;\n}\n@media screen and (max-width: 600px) {\n  #video-small-screen {\n    display: block;\n    @include screen_style;\n  }\n  #video-big-screen {\n    display: none;\n  }\n}\n\n/* end of slider video */\n\n.shopping-bag {\n  max-height: 75vh;\n  width: 0px;\n  position: absolute;\n  top: 71px;\n  right: 0px;\n  background-color: rgb(255, 254, 254);\n  transition: 0.3s linear;\n  border-radius: 3px;\n  box-shadow: 0px 0px 7px 1px rgb(183, 191, 198);\n  font-size: 16px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  overflow-y: scroll;\n\n  .empty-cart {\n    max-width: 500px;\n    min-width: 250px;\n    text-align: center;\n    font-size: x-large;\n    margin: 50px 0px 50px 0px;\n  }\n\n  .payment {\n    order: 1000;\n    margin: 50px 0px 50px 0px;\n    display: grid;\n    grid-gap: 10px;\n    justify-content: center;\n    align-items: center;\n    display: none;\n\n    .total-amount {\n      border: 1px solid rgb(164, 164, 164);\n      text-align: center;\n      font-size: 18px;\n      border: none;\n      border-top: 1.5px solid rgb(109, 109, 109);\n      border-bottom: 1.5px solid rgb(109, 109, 109);\n      padding: 13px 0px 13px 0px;\n      width: 250px;\n    }\n    .pay-btn {\n      width: 250px;\n      @include btn_style;\n    }\n  }\n}\n\n.show-bag {\n  width: 500px;\n}\n\n.selected-div {\n  width: 440px;\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  margin: 10px;\n\n  .selected-img {\n    width: 70px;\n  }\n\n  .selected-price {\n    width: 70px;\n    text-align: center;\n  }\n  .selected-quantity {\n    width: 40px;\n    text-align: center;\n  }\n  .plus,\n  .minus,\n  .remove {\n    border: 1px solid rgb(182, 182, 182);\n    padding: 0px 3px 0px 3px;\n    border-radius: 5px;\n    cursor: pointer;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n\n    &:hover {\n      border: 1px solid black;\n    }\n  }\n}\n\n@media screen and (max-width: 500px) {\n  .show-bag,\n  .empty-cart,\n  .selected-div {\n    max-width: 100vw;\n  }\n}\n/*** end of nav ****/\n\n/* product container */\n\n.product-container {\n  padding: 2vw;\n  min-height: fit-content;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(230px, 1fr));\n  justify-content: center;\n}\n/* end of product container */\n\n/*  product */\n\n.product {\n  margin: 0px 5px 50px 5px;\n  cursor: pointer;\n\n  .product-text {\n    font-size: large;\n    color: rgb(46, 46, 46);\n    background-color: white;\n    padding: 10px 20px 15px 0px;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n\n    .text-model {\n      margin-right: 70px;\n    }\n  }\n\n  .product-img {\n    width: 100%;\n    margin: auto;\n    border-radius: 3px;\n  }\n}\n/* end of  product */\n\n/*********** modal *************/\n.modal {\n  width: 100vw;\n  height: fit-content;\n  display: flex;\n  flex-wrap: wrap;\n  padding: 5vh 0vw 5vh 0vw;\n  background-color: white;\n  display: none;\n}\n\n.image-container {\n  flex: 1 0 70vw;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  grid-gap: 10px;\n\n  & img,\n  & video {\n    width: 100%;\n    margin: auto;\n  }\n}\n\n.content-container {\n  flex: 1 0 30vw;\n  height: 100vh;\n}\n\n.x-btn {\n  position: absolute;\n  top: 13vh;\n  right: 6vh;\n  font-size: 30px;\n  cursor: pointer;\n  color: rgb(104, 104, 104);\n}\n\n.modal-text-container {\n  margin: auto;\n  margin-top: 10vh;\n  margin-bottom: 5vh;\n  font-size: large;\n  width: fit-content;\n  display: flex;\n  flex-wrap: wrap;\n  align-items: center;\n\n  .modal-model {\n    margin-right: 100px;\n  }\n}\n\n.size-container {\n  width: fit-content;\n  height: fit-content;\n  margin: auto;\n  display: grid;\n  grid-gap: 10px;\n  grid-template-columns: repeat(4, 1fr);\n  grid-template-rows: repeat(4, 1fr);\n\n  & div {\n    padding: 15px;\n    border: 1px solid rgb(187, 187, 187);\n    @include flex_center;\n    cursor: pointer;\n\n    &:hover {\n      border-color: black;\n    }\n  }\n}\n\n.add-btn {\n  @include flex_center;\n  margin: 0 auto;\n  margin-top: 50px;\n  width: 300px;\n  @include btn_style;\n}\n\n.inactive {\n  background-color: rgb(70, 70, 70);\n  & {\n    cursor: initial;\n  }\n}\n/*********** end of modal *************/\n\n/* footer */\n.footer {\n  @include nav_and_footer;\n  justify-content: center;\n\n  .fa-github {\n    margin-left: 20px;\n    font-size: 30px;\n    transition: 0.3s linear;\n    @include icon_hover;\n  }\n}\n\n/* enf of footer */\n","sass/_resets.scss":"* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: 'Open Sans';\n  border-radius: 3px;\n  outline: none;\n}\n"},"lineCount":null}},"error":null,"hash":"b99b6be06fbb14e2f07ec9757bcfb6a7","cacheData":{"env":{}}}